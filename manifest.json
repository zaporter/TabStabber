{
    "manifest_version": 2,
    "name": "TabStabber",
    "version": "1.0",
    "description": "Kill those tabs",
    "permissions": [
        "tabs",
        "storage",
        "activeTab"
    ],
    "icons": {
        "48": "icons/border-48.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["main.js"]
        }
    ]
}
